<html t:type="layout" title="${message:report}"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

	<head>
	
	</head>
	<body> <t:form>
		<div class="block">
			<div class="newspaper-a">
				
				<fieldset>
					<legend class="legendTitle">${message:retiredEmployeeReportList}</legend>
					<table class="searchField">
						<t:security.hasPermission permission="show_all_organization">
							<tr>
								<td>${message:name}:</td>
							</tr>
							<tr>
								<td>
									<t:select t:model="organizationSelectionModel" t:value="org" t:blankLabel="${message:all}" t:style="width: 450px"/>
									&nbsp; <t:submit t:id="search" class="btnFind search" t:value="${message:search}"/>
								</td>
							</tr>
						</t:security.hasPermission>
					</table><br/>
						<t:grid t:source="listRetiredEmployee" t:row="valueRetiredEmployee" t:add="name,countOfficer"
							width="50%" rowClass="Gridrowcss">
							<p:nameCell>
								${quitCauseName}
							</p:nameCell>
							<p:countOfficerCell>
								${countEdu}
							</p:countOfficerCell>
							<p:empty>
								${message:noData}
							</p:empty>
						</t:grid>
						<p align="center">
						<t:actionlink t:id="export">
							<img src="${imgExcel}"/></t:actionlink>
						</p>
					</fieldset>	
				
			</div>
		</div>
	</t:form> </body> 
</html>
