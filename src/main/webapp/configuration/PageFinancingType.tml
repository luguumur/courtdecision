<html t:type="layoutAdmin" title="literal:Configuration"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
	<head>
	
	</head>
	<body> <t:form>
		
			<div>
				Санхүүжилтын төрөл: <t:textfield value="financingType.name" t:validate="required" style="width: 200px"/> &nbsp;
				<t:submit t:id="save" t:value="${message:save}" class="btnFind save"  />
			</div><br/>
			<div>
				<t:grid t:source="listFinancingType" t:row="valueFinancingType" t:include="name" t:add="number,edit,delete"
				 rowsPerPage="15" reOrder="number" >
				 	<p:numberCell>
						${number}
					</p:numberCell>
					<p:editCell>
						<t:actionlink t:id="edit" t:context="valueFinancingType">
							<img src="${editIcon}"/>
						</t:actionlink>
					</p:editCell>
					<p:deleteCell>
						<t:actionlink t:id="delete" t:context="valueFinancingType">
							<img src="${deleteIcon}"/>
						</t:actionlink>
					</p:deleteCell>
				</t:grid> 
			</div>
		
	</t:form> </body> 
</html>
