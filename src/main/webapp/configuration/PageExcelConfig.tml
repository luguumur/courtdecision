<html t:type="layoutCD" title="${message:configuration}"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">

<body class="loader">
	<t:form>
		<table class="form">
			<tr>
				<td colspan="2"><label>${message:organization}</label> <t:select
						t:id="org" t:model="organizationSelectionModel"
						t:value="organization" t:validate="required"
						onchange="form.submit();" /></td>
				<td colspan="2"><label>${message:type} </label> <t:select
						t:model="excelTypeSelectionModel" t:value="excelType"
						t:blankLabel="${message:Choose}" onchange="form.submit();" /></td>
			</tr>
			<tr>
				<td colspan="4">Анхааруулга!</td>
			</tr>
			<tr>
				<td colspan="4"><t:delegate to="activeBlock" /></td>
			</tr>
		</table>
	</t:form>
</body>

<t:block t:id="personalBlock">
	<t:excelImport.compExcelPersonal t:id="compExcelPersonal" />
</t:block>

