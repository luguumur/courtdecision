<html t:type="layoutAdmin" title="literal:Configuration"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
	<head>
	
	</head>
	<body> <t:form>
		
			<div>
				<t:textfield value="officiaryCategory.name" t:validate="required"/>
				
				
				<t:submit t:id="save" t:value="${message:save}"/>
			</div>
			<div>
				<t:grid t:source="listOfficiaryCategory"  t:row="valueOfficiaryCategory" t:include="id,name" t:add="edit,delete">
					<p:editCell>
						<t:actionlink t:id="edit" t:context="valueOfficiaryCategory">
							<img src="${editIcon}"/>
						</t:actionlink>
					</p:editCell>
					<p:deleteCell>
						<t:actionlink t:id="delete" t:context="valueOfficiaryCategory">
							<img src="${deleteIcon}"/>
						</t:actionlink>
					</p:deleteCell>
				</t:grid> 
			</div>
		
	</t:form> </body> 
</html>
