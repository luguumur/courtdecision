<html t:type="layoutCD" title="${message:configuration}"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">
<head>
<script>
	$(function() {
		$("#accordion").accordion({
			heightStyle : "content",
			autoHeight : false,
			collapsible : true,
			active : false
		});
	});
</script>
</head>

<body>
	<div class="accordBox">
		<div id="accordion">
			<h3>Албан тушаалын үүрэг</h3>
			<div>
				<t:zone t:id="occupationRoleFormZone" id="occupationRoleFormZone">
					<t:form t:zone="occupationRoleFormZone">
						<table class="form">
							<tr>
								<td><label>${message:OccupationRoleName}</label> <t:textfield
										value="occupationRole.name" /></td>
								<td><label>Удирдах эсэх</label> <t:checkbox
										t:value="occupationRole.isUdirdah" /></td>
								<td><t:submit t:id="saveOccRole" t:value="${message:save}" />
								</td>
							</tr>
						</table>
					</t:form>
				</t:zone>
				<t:zone t:id="occupationRoleListZone" id="occupationRoleListZone">
					<t:grid t:source="listOccupationRole" t:row="valueOccupationRole"
						t:include="name" t:add="occupationNumber,edit,delete"
						inplace="true" rowsPerPage="10" reOrder="occupationNumber">
						<p:occupationNumberCell>
							${occupationNumber}
						</p:occupationNumberCell>
						<p:editCell>
							<t:actionlink t:id="editOccRole" t:context="valueOccupationRole"
								t:zone="occupationRoleFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteOccRole"
								t:context="valueOccupationRole" t:zone="occupationRoleFormZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Албан тушаалын төрөл</h3>
			<div>
				<t:zone t:id="occupationTypeFormZone" id="occupationTypeFormZone">
					<t:form t:zone="occupationTypeFormZone">
						<table class="form">
							<tr>
								<td><t:textfield t:value="occupationType.name" /></td>
								<td><t:submit t:id="saveOccupationType"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="occupationTypeListZone" id="occupationTypeListZone">
					<t:grid t:source="listOccupationType" t:row="valueOccupationType"
						t:include="name" t:add="occupationTypeNumber,edit,delete"
						reOrder="occupationTypeNumber">
						<p:occupationTypeNumberCell>
							${occupationTypeNumber}
						</p:occupationTypeNumberCell>
						<p:editCell>
							<t:actionlink t:id="editOccupationType"
								t:context="valueOccupationType" t:zone="occupationTypeFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteOccupationType"
								t:context="valueOccupationType" t:zone="occupationTypeListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Албан тушаал</h3>
			<div>
				<t:zone t:id="appointmentFormZone" id="appointmentFormZone">
					<t:form t:zone="appointmentFormZone">
						<table class="form">
							<tr>
								<td><label>${message:appointment}</label> <t:select
										t:model="occupationTypeSelectionModel"
										t:value="appointment.occupationType" t:validate="required" />
								</td>
								<td><t:textfield t:value="appointment.AppointmentName"
										t:validate="required" /></td>
								<td><label>${message:guitsetgehAjliinNemegdel}</label> <t:checkbox
										t:value="appointment.gaNemegdel" /></td>
								<td></td>
								<td><t:submit t:id="saveAppointment"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>
				<t:zone t:id="appointmentListZone" id="appointmentListZone">
					<t:grid t:source="listAppointment" t:row="valueAppointment"
						t:include="appointmentName"
						reOrder="number,rowId,occupationTypeName,appointmentName,guitsetgehAjliinNemegdel"
						t:add="number,guitsetgehAjliinNemegdel,edit,delete,occupationTypeName,rowId"
						rowsPerPage="10" inPlace="true">
						<p:numberCell>
							${number}
						</p:numberCell>
						<p:rowIdCell>
							${valueAppointment?.id}
						</p:rowIdCell>
						<p:occupationTypeNameCell>
							${valueAppointment?.occupationType?.name}
						</p:occupationTypeNameCell>
						<p:guitsetgehAjliinNemegdelCell>
							${guitsetgehAjliinNemegdel}
						</p:guitsetgehAjliinNemegdelCell>
						<p:editCell>
							<t:actionlink t:id="editAppointment" t:context="valueAppointment"
								t:zone="appointmentFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteAppointment"
								t:context="valueAppointment" t:zone="appointmentListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Цалинжих хэлбэр</h3>
			<div>
				<t:zone t:id="salaryNetworkFormZone" id="salaryNetworkFormZone">
					<t:form t:zone="salaryNetworkFormZone">
						<table class="form">
							<tr>
								<td>${message:salaryNetworkName} <t:textfield
										t:value="salaryNetwork.salaryNetworkName"
										t:validate="required" />
								</td>
								<td><t:select t:model="wageSelectionMoel"
										t:value="salaryNetwork.getWage" t:validate="required" /></td>
								<td><t:submit t:id="saveSalaryNetwork"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="salaryNetworkListZone" id="salaryNetworkListZone">
					<t:grid t:source="listSalaryNetwork" t:row="valueSalaryNetwork"
						t:include="salaryNetworkName"
						t:add="salaryNetworkNumber,edit,delete" rowsPerPage="10"
						reOrder="salaryNetworkNumber">
						<p:salaryNetworkNumberCell>
					 		${salaryNetworkNumber}
					 	</p:salaryNetworkNumberCell>
						<p:editCell>
							<t:actionlink t:id="editSalaryNetwork"
								t:context="valueSalaryNetwork" t:zone="salaryNetworkFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteSalaryNetwork"
								t:context="valueSalaryNetwork" t:zone="salaryNetworkListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Цалингийн шатлал</h3>
			<div>
				<t:zone t:id="salaryScaleFormZone" id="salaryScaleFormZone">
					<t:form t:zone="salaryScaleFormZone">
						<table class="form">
							<tr>
								<td><label>${message:salaryScale}</label> <t:textfield
										t:value="salaryScale.salaryScale" t:validate="required" /></td>
								<td><t:submit t:id="saveSalaryScale"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="salaryScaleListZone" id="salaryScaleListZone">
					<t:grid t:source="listSalaryScale" t:row="valueSalaryScale"
						t:include="salaryScale" t:add="salaryScaleNumber,edit,delete"
						rowsPerPage="10" reOrder="salaryScaleNumber">
						<p:salaryScaleNumberCell>
					 		${salaryScaleNumber}
					 	</p:salaryScaleNumberCell>
						<p:editCell>
							<t:actionlink t:id="editSalaryScale" t:context="valueSalaryScale"
								t:zone="salaryScaleFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteSalaryScale"
								t:context="valueSalaryScale" t:zone="salaryScaleListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Шилжих шалтгааны төрөл</h3>
			<div>
				<t:zone t:id="displacementCauseFormZone"
					id="displacementCauseFormZone">
					<t:form t:zone="displacementCauseFormZone">
						<table class="form">
							<tr>
								<td><label>Төрөл</label> <t:textfield
										value="displacementCause.name" t:validate="required" /></td>
								<td><t:submit t:id="saveDisplacementCause"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="displacementCauseListZone"
					id="displacementCauseListZone">
					<t:grid t:source="listDisplacementCause"
						t:row="valueDisplacementCause" t:include="name"
						t:add="displacementCauseNumber,edit,delete" rowsPerPage="25"
						reOrder="displacementCauseNumber">
						<p:displacementCauseNumberCell>
					 		${displacementCauseNumber}
					 	</p:displacementCauseNumberCell>
						<p:editCell>
							<t:actionlink t:id="editDisplacementCause"
								t:context="valueDisplacementCause"
								t:zone="displacementCauseFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteDisplacementCause"
								t:context="valueDisplacementCause"
								t:zone="displacementCauseListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Чөлөөлөгдөх шалтгааны төрөл</h3>
			<div>
				<t:zone t:id="quitJobCauseFormZone" id="quitJobCauseFormZone">
					<t:form t:zone="quitJobCauseFormZone">
						<table class="form">
							<tr>
								<td><label>Чөлөөлөгдөх шалтгааны төрөл</label> <t:select
										t:model="QuitTypeSelectionModel"
										t:value="quitJobCause.causeType" t:validate="required" /></td>
								<td><label>${message:cause}</label> <t:textfield
										t:value="quitJobCause.causeName" t:validate="required" /></td>
								<td><t:submit t:id="saveQuitJobCause"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="quitJobCauseListZone" id="quitJobCauseListZone">
					<t:grid t:source="listQuitJobCause" t:row="valueQuitJobCause"
						t:include="causeType,causeName"
						t:add="quitJobCauseNumber,edit,delete"
						reOrder="quitJobCauseNumber" rowsPerPage="10">
						<p:quitJobCauseNumberCell>
					 		${quitJobCauseNumber}
					 	</p:quitJobCauseNumberCell>
						<p:quitJobCauseCell>
							${quitJobCauseNumber}
						</p:quitJobCauseCell>
						<p:commandSubjectCell>
							${valueQuitJobCause?.causeType?.name}
						</p:commandSubjectCell>
						<p:editCell>
							<t:actionlink t:id="editQuitJobCause"
								t:context="valueQuitJobCause" t:zone="quitJobCauseFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteQuitJobCause"
								t:context="valueQuitJobCause" t:zone="quitJobCauseListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>

			</div>
			<h3>Цалингийн сүлжээний тохиргоо</h3>
			<div>
				<t:zone t:id="salaryNetworkConfigFormZone"
					id="salaryNetworkConfigFormZone">
					<t:form t:zone="salaryNetworkConfigFormZone">
						<table class="form">
							<tr>
								<td><label>Төрийн захиргааны зэрэглэл</label> <t:select
										t:model="levelSelectionModel" t:value="salary.level"
										t:validate="required" /></td>
								<td><label>Цалингийн сүлжээний шатлал</label> <t:select
										t:model="salaryPhaseSelectionModel" t:value="salary.phase" />
								</td>
								<td><label><span class="red">*</span>Цалингийн дүн</label>
									<t:textfield t:value="salary.amount" t:validate="required" /></td>
								<td><label><span class="red">*</span>Огноо</label> <t:datefield
										t:value="salary.date" t:format="yyyy-MM-dd"
										t:validate="required" /></td>
							</tr>
							<tr>
								<td><label><span class="red">*</span>Хүчинтэй
										хугацаа/жилээр/</label> <t:textfield t:value="salary.validTime"
										t:validate="required" /></td>
								<td><label>Идэвхитэй</label> <t:checkbox
										t:value="salary.active" /></td>
								<td></td>
								<td><t:submit t:id="saveSalary" class="button"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="salaryNetworkConfigListZone"
					id="salaryNetworkConfigListZone">
					<t:grid t:source="listTuriinZahirgaaSalaryNetwork"
						t:row="valueTuriinZahirgaaSalaryNetwork"
						t:add="salaryNetworkConfigNumber,edit,delete,levelName,salaryScaleName,dateName,amountName,validTimeName"
						rowsPerPage="25" t:include="id"
						t:reOrder="salaryNetworkConfigNumber,levelName,salaryScaleName,amountName,validTimeName,dateName"
						inPlace="true">
						<p:salaryNetworkConfigNumberCell>
							${salaryNetworkConfigNumber}
						</p:salaryNetworkConfigNumberCell>
						<p:levelNameCell>
							${valueTuriinZahirgaaSalaryNetwork?.level?.utTzTtTuSort?.name} - ${valueTuriinZahirgaaSalaryNetwork?.level?.utTzTtTuRank?.rank}
						</p:levelNameCell>
						<p:salaryScaleNameCell>
							${valueTuriinZahirgaaSalaryNetwork?.phase?.salaryScale}
						</p:salaryScaleNameCell>
						<p:dateNameCell>
							${valueTuriinZahirgaaSalaryNetwork?.date}
						</p:dateNameCell>
						<p:amountNameCell>
							${valueTuriinZahirgaaSalaryNetwork?.amount}
						</p:amountNameCell>
						<p:validTimeNameCell>
							${valueTuriinZahirgaaSalaryNetwork?.validTime}
						</p:validTimeNameCell>
						<p:editCell>
							<t:actionlink t:id="editSalaryNetworkConfig"
								t:context="valueTuriinZahirgaaSalaryNetwork"
								t:zone="salaryNetworkConfigFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteSalaryNetworkConfig"
								t:context="valueTuriinZahirgaaSalaryNetwork"
								t:zone="salaryNetworkConfigListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Нэмэгдэл цалингийн төрөл</h3>
			<div>
				<t:zone t:id="additionalSalaryFormZone"
					id="additionalSalaryFormZone">
					<t:form t:zone="additionalSalaryFormZone">
						<table class="form">
							<tr>
								<td><label>${message:additionalSalaryType}</label> <t:textfield
										t:value="additionalSalary.name" t:validate="required" /></td>
								<td><label>${message:category}</label> <t:select
										t:model="categorySelectionModel"
										t:value="additionalSalary.category" t:validate="required" /></td>
								<td><t:submit t:id="saveAdditionalSalary"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="additionalSalaryListZone"
					id="additionalSalaryListZone">
					<t:grid t:source="listAdditionalSalaryType"
						t:row="valueAdditionalSalaryType" t:include="name,category"
						t:add="additionalSalaryNumber,edit,delete" rowsPerPage="10"
						reorder="additionalSalaryNumber">
						<p:additionalSalaryNumberCell>
							${additionalSalaryNumber}
						</p:additionalSalaryNumberCell>
						<p:editCell>
							<t:actionlink t:id="editAdditionalSalary"
								t:context="valueAdditionalSalaryType"
								t:zone="additionalSalaryFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteAdditionalSalary"
								t:context="valueAdditionalSalaryType"
								t:zone="additionalSalaryListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>

			</div>
			<h3>Тэтгэвэрт гарах нас</h3>
			<div>
				<t:zone t:id="retireAgeFormZone" id="retireAgeFormZone">
					<t:form t:zone="retireAgeFormZone">
						<table class="form">
							<tr>
								<td><label>${message:male}</label> <t:textfield
										t:value="retireAge.maleAge" t:validate="required" /></td>
								<td><label>${message:female}</label> <t:textfield
										value="retireAge.femaleAge" validate="required" /></td>
								<td><t:submit t:id="saveRetireAge"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="retireAgeListZone" id="retireAgeListZone">
					<t:grid t:source="listRetireAge" t:row="valueRetireAge"
						t:include="id,maleAge,femaleAge" t:add="edit">
						<p:maleAgeCell>
							${maleAge}
						</p:maleAgeCell>

						<p:femaleAgeCell>
							${femaleAge}
						</p:femaleAgeCell>
						<p:editCell>
							<t:actionlink t:id="editRetireAge" t:context="valueRetireAge"
								t:zone="retireAgeFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
					</t:grid>
				</t:zone>
			</div>

			<h3>Зэрэглэлийн ангилал</h3>
			<div>
				<t:zone t:id="occupationRankFormZone" id="occupationRankFormZone">
					<t:form t:zone="occupationRankFormZone">
						<table class="form">
							<tr>
								<td><label>Ангилал</label> <t:textfield
										t:value="occupationRank.rank" t:validate="required" /></td>
								<td></td>
								<td><t:submit t:id="saveOccupationRank"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="occupationRankListZone" id="occupationRankListZone">
					<t:grid t:source="listOccupationRank" t:row="valueOccupationRank"
						t:include="id" t:add="rankQuantity,edit,delete" rowsPerPage="10">
						<p:editCell>
							<t:actionlink t:id="editOccupationRank"
								t:context="valueOccupationRank" t:zone="occupationRankFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteOccupationRank"
								t:context="valueOccupationRank" t:zone="occupationRankListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
						<p:rankQuantityCell>
							${rank}
						</p:rankQuantityCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Улс</h3>
			<div>
				<t:zone t:id="countryFormZone" id="countryFormZone">
					<t:form t:zone="countryFormZone">
						<table class="form">
							<tr>
								<td><label>${message:country}</label> <t:textfield
										t:id="country" t:value="country.countryName"
										t:validate="required" /></td>
								<td><label>${message:language}</label> <t:textfield
										t:id="language" t:value="country.language"
										t:validate="required" /></td>
								<td><label>${message:isMongolia}</label> <t:checkbox
										value="country.isMongolia" /></td>
								<td><t:submit t:id="saveCountry" t:value="${message:save}" />
								</td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="countryListZone" id="countryListZone">
					<t:grid t:source="listCountry" t:row="valueCountry"
						t:include="countryName,language" t:add="countryNumber,edit,delete"
						rowsPerPage="10" reorder="countryNumber">
						<p:countryNumberCell>
							${countryNumber}
						</p:countryNumberCell>
						<p:editCell>
							<t:actionlink t:id="editCountry" t:context="valueCountry"
								t:zone="countryFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteCountry" t:context="valueCountry"
								t:zone="countryListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>

			</div>
			<h3>Хот /Гадаад улсын/</h3>
			<div>
				<t:zone t:id="cityProvinceFormZone" id="cityProvinceFormZone">
					<t:form t:zone="cityProvinceFormZone">
						<table class="form">
							<tr>
								<td><label>Хотын нэр /Гадаад улсын/</label> <t:select
										t:model="countrySelectionModel" value="cityProvince.country"
										t:validate="required" /></td>
								<td><label>${message:country}</label> <t:textfield
										value="cityProvince.cityProvinceName" t:validate="required" />

								</td>
								<td></td>
