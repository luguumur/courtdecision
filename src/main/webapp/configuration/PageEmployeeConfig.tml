								Засах
							</t:actionlink>
						</p:editCell>
						<p:editCell>
							<t:actionlink t:id="deleteRelativeType"
								t:context="valueRelativeType" t:zone="relativeTypeListZone">
								Устгах
							</t:actionlink>
						</p:editCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Гадаадад зорчсон чиглэл</h3>
			<div>
				<t:zone t:id="travelTypeFormZone" id="travelTypeFormZone">
					<t:form t:zone="travelTypeFormZone">
						<table class="form">
							<tr>
								<td><label>Гадаадад зорчсон чиглэл</label> <t:textfield
										t:value="travelType.name" t:validate="required" /></td>
								<td></td>
								<td><t:submit t:id="saveTravelType"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>
				<t:zone t:id="travelTypeListZone" id="travelTypeListZone">
					<t:grid t:source="listTravelType" t:row="valueTravelType"
						t:include="id,name" t:add="edit,delete">

						<p:editCell>
							<t:actionlink t:id="editTravelType" t:context="valueTravelType"
								t:zone="travelTypeFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteTravelType" t:context="valueTravelType"
								t:zone="travelTypeListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Гэр бүлийн хүний албан тушаалын төрөл</h3>
			<div>
				<t:zone t:id="FamilyAppointmentFormZone"
					id="FamilyAppointmentFormZone">
					<t:form t:zone="FamilyAppointmentFormZone">
						<table class="form">
							<tr>
								<td><label>Гэр бүлийн хүний албан тушаалын төрөл</label> <t:textfield
										t:value="familyAppointmentType.name" t:validate="required" />
								</td>
								<td></td>
								<td><t:submit t:id="saveFamilyAppointment"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="familyAppointmentListZone"
					id="familyAppointmentListZone">
					<t:grid t:source="listFamilyAppointmentType"
						t:row="valueFamilyAppointmentType" t:include="name"
						t:add="familyAppointmentNumber,edit,delete"
						reorder="familyAppointmentNumber">
						<p:familyAppointmentNumberCell>
							${familyAppointmentNumber}
						</p:familyAppointmentNumberCell>
						<p:editCell>
							<t:actionlink t:id="editFamilyAppointment"
								t:context="valueFamilyAppointmentType"
								t:zone="travelTypeListZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteFamilyAppointment"
								t:context="valueFamilyAppointmentType"
								t:zone="travelTypeFormZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Докторын зэрэг</h3>
			<div></div>
			<h3>Магистрын зэрэг</h3>
			<div>
				<t:zone t:id="ScienceDoctorFormZone" id="ScienceDoctorFormZone">
					<t:form t:zone="ScienceDoctorFormZone">
						<table class="form">
							<tr>
								<td><label>Нэр</label> <t:textfield
										t:value="scienceDoctor.name" t:validate="required" /></td>
								<td></td>
								<td><t:submit t:id="saveScienceDoctor"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="ScienceDoctorListZone" id="ScienceDoctorListZone">
					<t:grid t:source="listScienceDoctorType" t:row="valueScienceDoctor"
						t:include="name" t:add="number,edit,delete"
						reorder="number">
						<p:numberCell>
							${ScienceDoctorNumber}
						</p:numberCell>
						<p:editCell>
							<t:actionlink t:id="editScienceDoctor"
								t:context="valueScienceDoctor" t:zone="ScienceDoctorFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteScienceDoctor"
								t:context="valueScienceDoctor" t:zone="ScienceDoctorListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Цэргийн цол</h3>
			<div>
				<t:zone t:id="militaryRankFormZone" id="militaryRankFormZone">
					<t:form t:zone="militaryRankFormZone">
						<table class="form">
							<tr>
								<td><label>${message:Type}</label> <t:select
										t:model="militaryRankTypeSelectionModel"
										t:value="militaryRank.militaryRankType" validate="required" />
								</td>
								<td><label>${message:Military}</label> <t:textfield
										t:id="rank" t:value="militaryRank.militaryName"
										t:validate="required" /></td>
								<td><label>${message:periodTime}</label> <t:textfield
										t:id="period" t:value="militaryRank.period" /></td>
								<td><label>Тулах эсэх</label> <t:checkbox t:id="top"
										t:value="militaryRank.istop"></t:checkbox></td>
								<td><t:submit t:id="saveMilitaryRank"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="militaryRankListZone" id="militaryRankListZone">
					<t:grid t:source="listMilitaryRank" t:row="valueMilitaryRank"
						t:include="militaryRankType,militaryName"
						t:add="militaryRankNumber,periods,top,edit,delete"
						reorder="militaryRankNumber">
						<p:militaryRankNumberCell>
							${militaryRankNumber}
						</p:militaryRankNumberCell>
						<p:periodsCell>
							${period}
						</p:periodsCell>
						<p:topCell>
							${top}
						</p:topCell>
						<p:editCell>
							<t:actionlink t:id="editMilitaryRank"
								t:context="valueMilitaryRank" t:zone="militaryRankFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteMilitaryRank"
								t:context="valueMilitaryRank" t:zone="militaryRankListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Тушаал гаргасан субъект</h3>
			<div>
				<t:zone t:id="commandSubjectFormZone" id="commandSubjectFormZone">
					<t:form t:zone="commandSubjectFormZone">
						<table class="form">
							<tr>
								<td><label>${message:commandSubject}</label> <t:textfield
										t:value="commandSubject.subjectName" t:validate="required" />
								</td>
								<td></td>
								<td><t:submit t:id="saveCommandSubject"
										t:value="${message:save}" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>

				<t:zone t:id="commandSubjectListZone" id="commandSubjectListZone">
					<t:grid t:source="listCommandSubject" t:row="valueCommandSubject"
						t:include="subjectName"
						t:add="commandSubjectNumber,edit,delete,number"
						reorder="commandSubjectNumber">
						<p:commandSubjectNumberCell>
							${commandSubjectNumber}
						</p:commandSubjectNumberCell>
						<p:editCell>
							<t:actionlink t:id="editCommandSubject"
								t:context="valueCommandSubject" t:zone="commandSubjectFormZone">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteCommandSubject"
								t:context="valueCommandSubject" t:zone="commandSubjectListZone">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Тэтгэмж тусламж</h3>
			<div>
				<t:zone t:id="allowanceTypeFormZone" id="allowanceTypeFormZone">
					<t:form t:zone="allowanceTypeFormZone">
						<table class="form">
							<tr>
								<td>${message:allowanceTypeConfig} : <t:textfield
										value="allowanceType.allowanceTypeName" t:validate="required" />
									&nbsp;
								</td>
								<td></td>
								<td><t:submit t:id="saveAllowanceType"
										t:value="${message:save}" class="btnFind save" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>
				<t:zone t:id="allowanceTypeListZone" id="allowanceTypeListZone">
					<t:grid t:source="listAllowanceType" t:row="valueAllowanceType"
						t:include="allowanceTypeName" reorder="numberAllowanceType"
						t:add="numberAllowanceType,edit,delete" rowsPerPage="15">
						<p:numberAllowanceTypeCell>
							${numberAllowanceType}
						</p:numberAllowanceTypeCell>
						<p:editCell>
							<t:actionlink t:id="editAllowanceType"
								t:context="valueAllowanceType">
								Засах
							</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteAllowanceType"
								t:context="valueAllowanceType">
								Устгах
							</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
			<h3>Чөлөө</h3>
			<div>
				<t:zone t:id="vacationTypeFormZone" id="vacationTypeFormZone">
					<t:form t:zone="vacationTypeFormZone">
						<table class="form">
							<tr>
								<td>Чөлөө хүсэх төрөл : <t:textfield
										value="vacationType.vacationTypeName" t:validate="required" />
									&nbsp;
								</td>
								<td></td>
								<td><t:submit t:id="saveVacationType"
										t:value="${message:save}" class="btnFind save" /></td>
							</tr>
						</table>
					</t:form>
				</t:zone>
				<t:zone t:id="vacationTypeListZone" id="vacationTypeListZone">
					<t:grid t:source="listVacationType" t:row="valueVacationType"
						t:include="vacationTypeName" reorder="numberVacationType"
						t:add="numberVacationType,edit,delete" rowsPerPage="15">
						<p:numberVacationTypeCell>
						${numberVacationType}
					</p:numberVacationTypeCell>
						<p:editCell>
							<t:actionlink t:id="editVacationType"
								t:context="valueVacationType">
							Засах
						</t:actionlink>
						</p:editCell>
						<p:deleteCell>
							<t:actionlink t:id="deleteVacationType"
								t:context="valueVacationType">
							Устгах
						</t:actionlink>
						</p:deleteCell>
					</t:grid>
				</t:zone>
			</div>
		</div>
	</div>
</body>
</html>
